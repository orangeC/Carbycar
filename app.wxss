/**app.wxss**/

page {
  height: 100%;
  width: 100%;
  overflow: hidden;
  background-color: #eeedf3;
  margin: 0;
}

@font-face {
  font-family: 'iconfont'; /* project id 201947 */
  src: url('//at.alicdn.com/t/font_8hj2a4jdcsy06bt9.eot');
  src: url('//at.alicdn.com/t/font_8hj2a4jdcsy06bt9.eot?#iefix') format('embedded-opentype'),
         url('//at.alicdn.com/t/font_8hj2a4jdcsy06bt9.woff') format('woff'),
         url('//at.alicdn.com/t/font_8hj2a4jdcsy06bt9.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_8hj2a4jdcsy06bt9.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
}

