}

.icon-camera:before {
  content: "\e609";
  color: #626262;
  font-size: 1.1em;
}

.icon-info:before {
  content: "\e607";
  color: #626262;
  font-size: 1.1em;
}

.icon-smallTime:before {
  font-size: 0.7em;
  color: #999;
  content: "\e66a";
}

.icon-phone:before {
  font-size: 1.5em;
  color: #767676;
  content: "\e7eb";
}

.icon-star:before {
  font-size: 1em;
  color: #feb200;
  content: "\e627";
  padding-right: 10rpx;
}
