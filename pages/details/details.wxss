/* pages/details/details.wxss */

page {
  width: 100%;
  overflow: hidden;
  background-color: #eaeeef;
}

.container {
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  font-family: '黑体';
  color: #747474;
  font-size: 0.8em;
}

.normal {
  width: 100%;
  padding: 22rpx 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #3f3f41;
  font-size: 1.15em;
  background-color: #ebeff0;
}

.flexLeft {
  flex: 0.1;
  margin-right: 10rpx;
}

.flexCenter {
  flex: 1.3;
  display: flex;
  flex-direction: column;
}

.flexRight {
  color: #8e8f93;
  flex: 0.18;
  text-align: right;
}

.flexRight-right {
  color: #8e8f93;
  flex: 0.18;
}

.blue {
  color: #3089dd;
}

.black {
  color: #636363;
}

.header {
  width: 100%;
  display: flex;
  padding: 25rpx 30rpx;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
}

.header>view {
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #e8eced;
}

.header-text {
  color: #ff722b;
}

.publishTime {
  width: 100%;
  display: flex;
  padding: 15rpx 30rpx;
  margin-top: 15rpx;
  justify-content: flex-start;
  align-items: center;
  background-color: #fff;
}

.publishBid {
  padding: 3rpx 0;
  font-size: 1.1em;
  color: #fff;
  background-color: #ff732c;
  border: 3px solid #fad0b8;
  border-radius: 9px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.publishPri {
  padding: 3rpx 0;
  font-size: 1.1em;
  color: #fff;
  background-color: #60b2ec;
  border: 3px solid #c9e3f2;
  border-radius: 9px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.site {
  width: 100%;
  display: flex;
  padding: 18rpx 30rpx;
  justify-content: space-between;
  align-items: center;
  font-size: 1.15em;
  background-color: #f8f8f8;
  color: #424242;
}

.siteLeft {
  display: flex;
}

.siteRight {
  font-size: 0.85em;
}

.siteRight-text {
  font-size: 1.2em;
  color: #fd7028;
}

.tips {
  width: 100%;
  padding: 15rpx 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #3089dd;
  background-color: #fff;
}

.circleOne {
  position: relative;
  width: 6px;
  height: 6px;
  background-color: #348adf;
  border: 2px solid #c6dcf1;
  border-radius: 50%;
}

.carInfoRight {
  font-size: 0.55em;
  font-weight: 900;
}

.carDetail {
  width: 100%;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: flex-start;
  color: #3f3f41;
  font-size: 1em;
  background-color: #fff;
  border-top: 1px solid #ececec;
}

.carDetail-text {
  padding-top: 8rpx;
  font-size: 0.9em;
  color: #979797;
}

.consignInfoX {
  width: 100%;
  padding: 20rpx 30rpx;
  display: flex;
  flex-direction: column;
  color: #989898;
  font-size: 0.9em;
  background-color: #fff;
}

.consignInfoX>text {
  padding-top: 8rpx;
}

.consignInfoX-head {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.consignInfoX-car {
  margin-right: 15rpx;
  padding: 8rpx 30rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #fc7229;
  font-size: 1em;
  border: 1px solid #fc7229;
  border-radius: 25px;
}

.dark {
  color: #989898;
  border: 1px solid #989898;
}

.consignInfoX-site {
  padding-top: 8rpx;
}

.consignInfoX-name {
  color: #328bdd;
}

.pricingH {
  width: 100%;
  padding: 15rpx 30rpx;
  display: flex;
  flex-direction: column;
  color: #989898;
  font-size: 0.9em;
  background-color: #fff;
}

.pricingH>view {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 7rpx 0;
}

.pricingH-topText {
  font-size: 1.2em;
  color: #000;
}

.pricingC-header {
  width: 33%;
  display: flex;
  align-items: center;
}

.pricingC>text {
  width: 33%;
}

.pricingC-center {
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-right: 1px solid #eaeeef;
  border-left: 1px solid #eaeeef;
}

.pricingC-right {
  text-align: right;
  font-size: 1em;
  color: #ff7229;
}

.pricingB {
  width: 100%;
  padding: 20rpx 30rpx;
  margin-bottom: 10rpx;
  background-color: #f8f8f8;
  color: #989898;
  font-size: 0.9em;
  box-shadow: 0px 1px 1px #c0c0c0;
}

.pricingB>text {
  color: #727272;
}

.acceptInfoX {
  width: 100%;
  padding: 20rpx 30rpx;
  display: flex;
  flex-direction: column;
  color: #989898;
  font-size: 0.9em;
  background-color: #fff;
}

.acceptInfoX>view {
  padding-top: 6rpx;
}

.getCarX {
  width: 100%;
  padding: 0rpx 30rpx;
  display: flex;
  flex-direction: column;
  color: #989898;
  font-size: 0.9em;
  background-color: #fff;
}

.getCarX>view {
  padding: 10rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.getCarX text {
  padding-top: 8rpx;
}

.getCarX-header {
  display: flex;
  flex-direction: column;
}

.certificateX {
  width: 100%;
  padding: 30rpx 30rpx;
  display: flex;
  justify-content: space-around;
  color: #989898;
  font-size: 1em;
  background-color: #fff;
}

.certificateX>view {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.noImage {
  width: 75px;
  height: 115px;
  border: 1px dashed #d3d3d3;
  background-color: #f3f3f3;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  color: #bfbfbf;
  font-size: 0.9em;
  padding-top: 20rpx;
}

.noImageCamera {
  padding: 20rpx 0;
  font-size: 2.3em;
}

.certificateX image {
  width: 75px;
  height: 125px;
  border: 1px dashed #d3d3d3;
}

.certificateX text {
  padding-top: 15rpx;
}

.certificateX-circle {
  position: absolute;
  top: -6%;
  right: -8%;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  background-color: #fc762d;
  text-align: center;
  color: #fff;
  line-height: 20px;
}

.swiper-container {
  margin: 0 auto;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #000;
}

swiper-item {
  display: flex;
  justify-content: center;
  align-items: center;
}

.swiper-container image {
  width: 70%;
  height: 70%;
}

.icon-time:before {
  font-size: 0.8em;
  color: #ff722a;
  content: "\e66a";
  margin: 0 5px 0 15px;
}

.icon-arrow:before {
  content: "\e603";
  color: #ea8e4d;
  font-size: 1.1em;
  margin: 0 10rpx;
}

.icon-car:before {
  content: "\e613";
  color: #656766;
  font-size: 1em;
}

.icon-smallCar:before {
  content: "\e65e";
  color: #626262;
  font-size: 1em;
}

.icon-truck:before {
  content: "\e608";
  color: #656567;
  font-size: 1.05em;
}

.icon-gift:before {
  content: "\e646";
  color: #656565;
  font-size: 1.05em;
}

.icon-yes-dark:before {
  content: "\e619";
  color: #989898;
  font-size: 0.9em;
  margin-right: 5px;
}

.icon-yes-light:before {
  content: "\e619";
  color: #fe712a;
  font-size: 0.9em;
  margin-right: 5px;
}

.icon-bill-light:before {
  content: "\e60e";
  color: #fe712a;
  font-size: 0.9em;
  margin-right: 5px;
}

.icon-bill-dark:before {
  content: "\e60e";
  color: #989898;
  font-size: 0.9em;
  margin-right: 5px;
}

.icon-site:before {
  content: "\e6cb";
  color: #686765;
