/* pages/publish/consignCar/consignCar.wxss */
.noWarning{
    padding: 30rpx 0;
    background-color: #ebeeef
}
.warning{
    display: flex;
    justify-content: center;
    padding: 15rpx 0;
    font-size: 10pt;
    background-color: lightgoldenrodyellow;
    color: orange;
}
.container{
    background-color: #ffffff;
    color: #999;
    font-size: 0.875em;
}
.container>view{
    display: flex;
    flex-direction: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 30rpx;
}
.horn{
  float: right;
  color: #999
}
.font{
    color: #444;
}
.brand{
    line-height: 80rpx;
    border-bottom: 1rpx solid #ebeeef
}
.newCar{
    line-height: 120rpx;
    border-bottom: 1rpx solid #ebeeef
}
.need{
    line-height: 60rpx;
    padding: 20rpx 0
}
.input{
    text-align: right
}
.carStatus>view{
    padding: 1rpx 0;
}
.icon-yes-dark:before {
  content: "\e6ba";
  color: #989898;
  font-size: 0.875em;
}

.icon-yes-light:before {
  content: "\e6ba";
  color: #fe712a;
  font-size: 0.875em;
}
button {
  background-color: #368cdf;
  color: #fff;
}
.consignInfoX-car {
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #fc7229;
  border: 1px solid #fc7229;
  border-radius: 25px;
}
.consignInfoX-car>text{
  font-size: 1em;
  padding: 0 30rpx
}
.dark {
  color: #989898;
  border: 1px solid #989898;
}